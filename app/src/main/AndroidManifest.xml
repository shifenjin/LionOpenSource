<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lion_personal.lionopensource">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />

        <service
            android:name=".MyService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".RxjavaActivity"></activity>
        <activity android:name=".OkhttpActivity" />
        <activity android:name=".retrofit.RetrofitActivity" />
        <activity android:name=".GlideActivity"></activity>
        <activity android:name=".database.SQLiteActivity" />
        <activity android:name=".data_structure.DataStructureActivity" />
        <activity android:name=".fragment.LionFragmentActivity" />
        <activity android:name=".recyclerview.RecyclerViewActivity">
        </activity>
        <activity android:name=".annotation.AnnotationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>